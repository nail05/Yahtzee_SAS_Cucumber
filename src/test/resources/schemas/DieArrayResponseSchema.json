{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "DieArrayResponseSchema": {
    "type": "object",
    "properties": {
      "status": {
        "type": "string"
      },
      "data": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "id": {
              "type": "integer"
            },
            "value": {
              "type": "integer"
            }
          },
          "required": ["id", "value"]
        }
      }
    },
    "required": ["status", "data"]
  }
}
